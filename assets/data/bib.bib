}

@article{YangCytochromeP4502E1Oxidative2012,
  title = {Cytochrome {{P4502E1}}, {{Oxidative Stress}}, {{JNK}} and {{Autophagy}} in {{Acute Alcohol}}-Induced {{Fatty Liver}}},
