@misc{3MFAQFluorinert,
  title = {{{3M}}\_{{FAQ}}\_{{Fluorinert}}.Pdf},
  urldate = {2017-11-29},
  url = {http://detector-cooling.web.cern.ch/detector-cooling/data/3M_FAQ_Fluorinert.pdf},
  file = {/home/edunn/Zotero/storage/S8L2S9VD/3M_FAQ_Fluorinert.pdf},
  note = {00000},
  annote = {Fluoride ion can be generated any time the fluid is brought above its decomposition temperature and this can lead to corrosion. Decomposition is generally very slow below 200-300$^\circ$C with most Fluorinert and HFE products. Experience indicates that corrosion resulting from decomposition is very rare and is only observed in systems which have undergone a failure.

Heaters should be sized such that their heat flux or power output does not exceed 6-10 W/cm2.

Boiling for both Fluorinerts and Novec HFEs generally begins at heat fluxes of around 1-2 W/cm2

The saturation level of water in a Fluorinert or Novec HFE fluid is usually less than 100 ppm by weight. Water present in the fluid at or below this level has never been linked to significant corrosion. Water present above this level means a separate water phase. If such water is entrained into contact with metal parts, corrosion can be rapid.

Water from these sources can be removed with any common molecular sieve or silica gel desiccant sold for refrigeration systems.

~}
}

@inproceedings{TumaDesignconsiderationsrelating2011,
  title = {Design Considerations Relating to Non-Thermal Aspects of Passive 2-Phase Immersion Cooling},
  doi = {10.1109/STHERM.2011.5767224},
  abstract = {There is renewed interest in passive 2-phase immersion for cooling power electronics and high performance computers. This can be attributed to recent research showing its performance potential compared with more complex and costly techniques, to innovations that simplify its application and to a general trend toward higher power densities. Though the thermal performance capabilities of passive 2-phase immersion cooling are well documented, the technique is not widely practiced and system designers will find little published information concerning subtler and very critical aspects of system design. There is no manual, for example, concerning practical details like material compatibility, electrical signal integrity (SI), fluid decomposition, management of moisture and light gases, and so on. This paper presents a useful material compatibility test method and explains the mechanisms of distillation and extraction that are intrinsic to a refluxing 2-phase system and by which wetted materials interact with the fluid and each other. It discusses sources, implications and techniques for removal of organic contaminants, water, non-condensable air and fluid thermal decomposition products. Data are presented from sub-20 GHz SI experiments conducted with backplane connectors and microstrip transmission lines submerged in two classes of environmentally sustainable working fluids. It is hoped that this overview will demystify these subjects for designers unfamiliar with passive 2-phase immersion cooling and encourage more widespread adoption of this elegant and proven technology.},
  booktitle = {2011 27th {{Annual IEEE Semiconductor Thermal Measurement}} and {{Management Symposium}}},
  author = {Tuma, P. E.},
  month = mar,
  year = {2011},
  keywords = {cooling,2-phase,backplane connectors,data center,electric connectors,electrical signal integrity,exascale,fluid decomposition,fluid thermal decomposition products,Fluids,fluoroketone,gate-turn-off thyristors,Hoses,hydrofluoroether,immersion,Immersion cooling,Insertion loss,light gases,material compatibility test method,microstrip lines,microstrip transmission lines,moisture management,open bath,organic contaminants,passive,passive 2-phase immersion cooling,power density,power electronic cooling,power electronics,Structural rings,system design,thyristors,transmission lines},
  pages = {1--9},
  file = {/home/edunn/Documents/zotero/undefined/2011/Tuma_2011_Design considerations relating to non-thermal aspects of passive 2-phase.pdf;/home/edunn/Zotero/storage/6S76XV4D/5767224.html},
  note = {00010},
  annote = {Decomposition products like PFIB can be removed in situ from PFC fluids to mitigate risk prior to servicing. The fluid is typically pumped through basic (pH\textasciitilde{}11) alumina granules that irreversibly destroy unsaturated compounds like PFIB as well as acids, free fluoride or hydrogen fluoride (HF) that may form 28. Phenolphthalein can be coated on the granules which will then change from pink to white as the pH drops due to destruction of decomposition products.

