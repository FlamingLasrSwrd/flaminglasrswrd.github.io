  number = {2},
  journal = {The Journal of Pharmacology and Experimental Therapeutics},
  author = {Castro, Patricio A. and Figueroa, Maximiliano and Yevenes, Gonzalo E. and San Martin, Loreto S. and Aguayo, Luis G.},
  month = feb,
  year = {2012},
  keywords = {ethanol,Humans,Cysteine,Neurotransmitter Agents,Dose-Response Relationship; Drug,Amino Acid Substitution,Cell Membrane,Chemical Phenomena,Drug Synergism,Electrophysiological Phenomena,Ethyl Methanesulfonate,Glycine,GTP-Binding Protein beta Subunits,GTP-Binding Protein gamma Subunits,Guanosine 5'-O-(3-Thiotriphosphate),HEK293 Cells,Ion Channel Gating,Lysine,Models; Molecular,Patch-Clamp Techniques,Propofol,Protein Interaction Domains and Motifs,Receptors; Glycine,Recombinant Fusion Proteins,Static Electricity,Surface Properties,Transfection},
