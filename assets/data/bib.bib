@article{obeid_trimethylamine-emphasis_2017,
  title = {Trimethylamine-$<${{Emphasis Type}}="{{Italic}}"$>${{N}}$<$/{{Emphasis}}$>$-Oxide and Its Biological Variations in Vegetarians},
