@article{VonkDigestionsocalledresistant2000,
  title = {Digestion of So-Called Resistant Starch Sources in the Human Small Intestine},
  volume = {72},
  issn = {0002-9165, 1938-3207},
  abstract = {Background: Resistant starch sources, which are only partially digested in the small intestine, can be used to increase colonic availability of short-chain fatty acids.
Objective: To study the characteristics of the fermentation of resistant starch, the digestion of resistant starch in the small intestine has to be quantified. We compared the metabolic fates of highly digestible cornstarch (DCS), Hylon VII (type 2 resistant starch), and Novelose 330 (type 3 resistant starch), which are of corn origin and, therefore, naturally enriched in 13C.
Design: After administration of 40 g starch or glucose to 7 healthy volunteers, glucose and exogenous glucose concentrations in serum and 13CO2 excretion in breath were analyzed for 6 h. 13C abundance in carbon dioxide was analyzed by isotope ratio mass spectrometry (IRMS) and 13C abundance in glucose by gas chromatography\textendash{}combustion IRMS.
Results: By comparing the area under the curve (2 h) of exogenous glucose concentration in serum (13C glycemic index) after intake of starch or glucose, 13C glycemic indexes for DCS, Hylon VII, and Novelose 330 were calculated to be 82 $\pm$ 23\%, 44 $\pm$ 16\%, and 43 $\pm$ 15\%, respectively. Comparison of 6-h cumulative percentage dose recovery in breath showed that 119 $\pm$ 28\% of DCS, 55 $\pm$ 23\% of Hylon VII, and 50 $\pm$ 26\% of Novelose 330 was digested in the small intestine.
Conclusion: The exogenous glucose response in serum and the 13CO2 excretion in breath can be used to estimate small intestinal digestion of resistant starch, which amounts to $\approx$50\%.},
