  journal = {Free Radical Biology and Medicine},
  author = {Bustamante, Juanita and Lodge, John K and Marcocci, Lucia and Tritschler, Hans J and Packer, Lester and Rihn, Bertrand H},
  month = apr,
  year = {1998},
  keywords = {Free Radicals,Liver,oxidative stress,Thioctic Acid,Î±-Lipoic acid,Disease,Primary biliary cirrhosis},
  pages = {1023--1039},
  file = {/home/edunn/Documents/zotero/Free Radical Biology and Medicine/1998/Bustamante et al_1998_Î±-Lipoic Acid in Liver Metabolism and Disease.pdf;/home/edunn/Zotero/storage/33539A54/S0891584997003717.html}
}

