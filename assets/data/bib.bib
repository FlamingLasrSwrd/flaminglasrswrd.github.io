  year = {2007},
  pages = {38--47},
  file = {/media/truecrypt1/zotero/Alcohol Research & Health/2007/Seitz_Becker_2007_Alcohol Metabolism and Cancer Risk.pdf},
  pmid = {17718399},
  pmcid = {PMC3860434}
}

