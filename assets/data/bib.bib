  month = may,
  year = {2008},
  file = {/media/truecrypt1/zotero/Addiction (Abingdon, England)/2008/Howland et al_2008_The Incidence and Severity of Hangover the Morning after Moderate Alcohol.pdf},
  pmid = {18412754},
