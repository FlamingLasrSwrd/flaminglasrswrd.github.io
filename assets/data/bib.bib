  journal = {The American Journal of Clinical Nutrition},
  author = {Vonk, Roel J. and Hagedoorn, Renate E. and de Graaff, Rynate and Elzinga, Henk and Tabak, Saskia and Yang, Yue-Xin and Stellaard, Frans},
  month = jan,
