  author = {Bertsimas, D. and Tsitsiklis, J.N.},
  year = {2015},
  keywords = {Agent-Based and Multiagent Systems,double oracle,Full Papers,game theory,icle,minimax,minimax equilib-,Technical Papers â€“ Multiagent Systems,Technical Papers â€” Game Theory,zero-sum games},
