  volume = {205},
  issn = {0926-3373},
  doi = {10.1016/j.apcatb.2016.12.045},
  abstract = {The non-oxidative dehydrogenation of ethanol to acetaldehyde has long been considered as an important method to produce acetaldehyde and clean hydrogen gas. Although monometallic Cu nanoparticles have high activity in the non-oxidative dehydrogenation of ethanol, they quickly deactivate due to sintering of Cu. Herein, we show that adding a small amount of Ni (Ni0.01Cu - Ni0.001Cu) into Cu to form highly dilute NiCu alloys dramatically increases the catalytic activity and increases their long-term stability. The kinetic studies show that the apparent activation energy decreases from $\sim$70kJ/mol over Cu to $\sim$45kJ/mol over the dilute NiCu alloys. The improved performance is observed both for nanoparticles and nanoporous NiCu alloys. The improvement in the long-term stability of the catalysts is attributed to the stabilization of Cu against sintering. Our characterization data show that Ni is atomically dispersed in Cu. The comparison of the catalytic performance of highly dilute alloy nanoparticles with nanoporous materials is useful to guide the design of novel mesoporous catalyst architectures for selective dehydrogenation reactions.},
  number = {Supplement C},
  urldate = {2017-11-15},
  url = {http://www.sciencedirect.com/science/article/pii/S0926337316309808},
  journal = {Applied Catalysis B: Environmental},
  author = {Shan, Junjun and Janvelyan, Nare and Li, Hang and Liu, Jilei and Egle, Tobias M. and Ye, Jianchao and Biener, Monika M. and Biener, Juergen and Friend, Cynthia M. and Flytzani-Stephanopoulos, Maria},
  month = may,
  year = {2017},
  keywords = {Acetaldehyde,Copper,Ethanol dehydrogenation,Hydrogen,Nickel,Single atom alloys},
  pages = {541--550},
  file = {/home/edunn/Documents/zotero/Applied Catalysis B Environmental/2017/Shan et al_2017_Selective non-oxidative dehydrogenation of ethanol to acetaldehyde and hydrogen.pdf;/home/edunn/Zotero/storage/F2HE5HWG/S0926337316309808.html}
}

