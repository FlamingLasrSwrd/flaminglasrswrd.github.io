@article{schwarzinger_contribution_2018,
  title = {Contribution of Alcohol Use Disorders to the Burden of Dementia in {{France}} 2008\textendash{}13: A Nationwide Retrospective Cohort Study},
  volume = {0},
  issn = {2468-2667},
  shorttitle = {Contribution of Alcohol Use Disorders to the Burden of Dementia in {{France}} 2008\textendash{}13},
  doi = {10.1016/S2468-2667(18)30022-7},
  abstract = {\subsection{Summary$<$/h2$>$

\subsubsection{Background$<$/h3$>$

Dementia is a prevalent condition, affecting 5\textendash{}7\% of people aged 60 years and older, and a leading cause of disability in people aged 60 years and older globally. We aimed to examine the association between alcohol use disorders and dementia risk, with an emphasis on early-onset dementia ($<$65 years).$<$/p$>$

\subsubsection{Methods$<$/h3$>$

We analysed a nationwide retrospective cohort of all adult ($\geq$20 years) patients admitted to hospital in metropolitan France between 2008 and 2013. The primary exposure was alcohol use disorders and the main outcome was dementia, both defined by International Classification of Diseases, tenth revision discharge diagnosis codes. Characteristics of early-onset dementia were studied among prevalent cases in 2008\textendash{}13. Associations of alcohol use disorders and other risk factors with dementia onset were analysed in multivariate Cox models among patients admitted to hospital in 2011\textendash{}13 with no record of dementia in 2008\textendash{}10.$<$/p$>$

\subsubsection{Findings$<$/h3$>$

Of 31 624 156 adults discharged from French hospitals between 2008 and 2013, 1 109 343 were diagnosed with dementia and were included in the analyses. Of the 57 353 (5$\cdot$2\%) cases of early-onset dementia, most were either alcohol-related by definition (22 338 [38$\cdot$9\%]) or had an additional diagnosis of alcohol use disorders (10 115 [17$\cdot$6\%]). Alcohol use disorders were the strongest modifiable risk factor for dementia onset, with an adjusted hazard ratio of 3$\cdot$34 (95\% CI 3$\cdot$28\textendash{}3$\cdot$41) for women and 3$\cdot$36 (3$\cdot$31\textendash{}3$\cdot$41) for men. Alcohol use disorders remained associated with dementia onset for both sexes (adjusted hazard ratios $>$1$\cdot$7) in sensitivity analyses on dementia case definition (including Alzheimer's disease) or older study populations. Also, alcohol use disorders were significantly associated with all other risk factors for dementia onset (all p$<$0$\cdot$0001).$<$/p$>$

\subsubsection{Interpretation$<$/h3$>$

Alcohol use disorders were a major risk factor for onset of all types of dementia, and especially early-onset dementia. Thus, screening for heavy drinking should be part of regular medical care, with intervention or treatment being offered when necessary. Additionally, other alcohol policies should be considered to reduce heavy drinking in the general population.$<$/p$>$

\subsubsection{Funding$<$/h3$>$

None.$<$/p$>$

}

}

}

}

}

}},
  language = {English},
  number = {0},
  urldate = {2018-02-21},
  url = {http://www.thelancet.com/journals/lanpub/article/PIIS2468-2667(18)30022-7/abstract},
  journal = {The Lancet Public Health},
  author = {Schwarzinger, Micha{\"e}l and Pollock, Bruce G. and Hasan, Omer S. M. and Dufouil, Carole and Rehm, J{\"u}rgen and Baillot, S. and Guibert, Q. and Planchet, F. and Luchini, S.},
