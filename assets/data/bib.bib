  month = jul,
  year = {2009},
  keywords = {Acetaldehyde,Ethyl acetate,Ethanol,Oxidation,Palladium,PdO/SiO,Silica},
  pages = {109--114},
  file = {/home/edunn/Documents/zotero/Applied Catalysis A General/2009/Gaspar et al_2009_Chemicals from ethanolâ€”The ethyl acetate one-pot synthesis.pdf;/home/edunn/Zotero/storage/RE38WGBL/S0926860X09003342.html}
