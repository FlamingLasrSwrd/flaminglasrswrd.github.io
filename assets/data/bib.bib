@article{SantacesariaEthanoldehydrogenationethyl2012,
  title = {Ethanol Dehydrogenation to Ethyl Acetate by Using Copper and Copper Chromite Catalysts},
  volume = {179},
  issn = {1385-8947},
  doi = {10.1016/j.cej.2011.10.043},
  abstract = {In the recent years, the interest in the ethanol production from renewable natural sources, as a possible alternative energy vector, has strongly grown in the world. The low-cost ethanol availability has also favored the study of the production of different chemicals such as ethylene, ethyl ether, acetaldehyde and ethyl acetate starting from ethanol as raw material. In this paper, ethanol dehydrogenation to ethyl acetate, in one step reaction, has been studied by using three different commercial copper based catalysts. The reaction has been conducted in a conventional packed bed tubular reactor, by exploring a temperature range of 200\textendash{}260$^\circ$C and a pressure range of 10\textendash{}30 bars. The best results have been found by using a commercial copper/copper chromite catalyst, supported on alumina and containing barium chromite as promoter, operating at 220\textendash{}240$^\circ$C, 20 bars and 98ghmol-1 of ethanol contact time. In these conditions, a conversion of 65\% with a selectivity to ethyl acetate of 98\textendash{}99\% has been obtained. However, the effect of temperature, pressure and ethanol contact time on both conversion and selectivity to ethyl acetate has been investigated. Moreover, the best catalyst has also shown a good stability to deactivation. For these reasons, the behavior of this catalyst has been subjected to a deeper investigation. The obtained results have been discussed on the basis of a reliable reaction scheme and mechanism. At last, a simplified scheme of a possible process is reported.},
  number = {Supplement C},
  urldate = {2017-11-15},
  url = {http://www.sciencedirect.com/science/article/pii/S1385894711012824},
  journal = {Chemical Engineering Journal},
  author = {Santacesaria, E. and Carotenuto, G. and Tesser, R. and Di Serio, M.},
  month = jan,
  year = {2012},
  keywords = {Bio-ethanol,Copper chromite catalyst,Dehydrogenation,Ethyl acetate},
  pages = {209--220},
  file = {/home/edunn/Documents/zotero/Chemical Engineering Journal/2012/Santacesaria et al_2012_Ethanol dehydrogenation to ethyl acetate by using copper and copper chromite2.pdf;/home/edunn/Zotero/storage/YEQ3MULH/1-s2.0-S1385894711012824-sc1.jpg;/home/edunn/Zotero/storage/YU74MCKQ/S1385894711012824.html},
  annote = {It is important to point out that operating at atmospheric pressure acetaldehyde is the main reaction product \href{https://www-sciencedirect-com.www2.lib.ku.edu/science/article/pii/S1385894711012824?_rdoc=1&amp;_fmt=high&amp;_origin=gateway&amp;_docanchor=&amp;md5=b8429449ccfc9c30159a5f9aeaa92ffb#bib0130}{[26,27]}, while the obtainment of ethyl acetate requires to operate under moderate pressures (20\textendash{}30~bars).}
}

@article{GasparChemicalsethanolethyl2009,
  title = {Chemicals from Ethanol\textemdash{}{{The}} Ethyl Acetate One-Pot Synthesis},
  volume = {363},
  issn = {0926-860X},
  doi = {10.1016/j.apcata.2009.05.001},
  abstract = {The direct synthesis of ethyl acetate from ethanol, a renewable raw material, was studied using PdO/SiO2 catalysts and the oxidative route. These catalysts were prepared by ionic exchange and also by impregnation. They were characterized by XPS, UV\textendash{}vis, XRD, chemical analysis and BET. These catalysts were tested under different experimental conditions using a conventional system with a fixed bed reactor at atmospheric pressure. It was inferred that the ethyl acetate synthesis occurs via ethanol oxidation followed by the condensation reaction of acetaldehyde and ethanol or ethoxide species. It can be suggested that the rate limiting step of the synthesis change according to the rate of ethanol consumption. For low rates, the limiting step is the ethanol oxidation to acetaldehyde, and for the high ones, it is the condensation reaction. It was verified that the catalysts preparation conditions modify the PdO distribution on the support and its particle sizes, therefore, changing the ethanol consumption rate, and consequently, the selectivities to acetaldehyde, ethyl acetate, acetic acid and CO2. This work shows that the ethyl acetate synthesis by the oxidative route using PdO/SiO2 catalyst can be considered as a very promising new process.},
  number = {1},
  urldate = {2017-11-15},
