  file = {/home/edunn/Documents/zotero/Nutrition Journal/2014/Ward_2014_Addressing nutritional gaps with multivitamin and mineral supplements.pdf},
  pmid = {25027766},
  pmcid = {PMC4109789},
  note = {00022}
}

@article{white_functional_2016,
  title = {Functional {{Brain Activity Changes}} after 4 {{Weeks Supplementation}} with a {{Multi}}-{{Vitamin}}/{{Mineral Combination}}: {{A Randomized}}, {{Double}}-{{Blind}}, {{Placebo}}-{{Controlled Trial Exploring Functional Magnetic Resonance Imaging}} and {{Steady}}-{{State Visual Evoked Potentials}} during {{Working Memory}}},
