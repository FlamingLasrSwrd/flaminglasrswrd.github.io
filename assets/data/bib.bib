  year = {2017},
  keywords = {Review},
  pages = {183--206},
  file = {/media/truecrypt1/zotero/Alcohol Research  Current Reviews/2017/Zahr_Pfefferbaum_2017_Alcoholâ€™s Effects on the Brain.pdf},
  pmid = {null},
  pmcid = {PMC5513685}
