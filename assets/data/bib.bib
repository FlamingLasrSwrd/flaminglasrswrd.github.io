  keywords = {in vitro,acute,rat,in vivo,5g/kg injection,NOS,primary},
  file = {/media/truecrypt1/zotero/International Journal of Molecular Sciences/2017/Horibe et al_2017_Upregulated Autophagy in Sertoli Cells of Ethanol-Treated Rats Is Associated.pdf},
