  journal = {BMJ : British Medical Journal},
  author = {Hendriks, H. F. and Veenstra, J. and Velthuis-te Wierik, E. J. and Schaafsma, G. and Kluft, C.},
  month = apr,
  year = {1994},
  pages = {1003--1006},
  file = {/home/edunn/Documents/zotero/BMJ  British Medical Journal/1994/Hendriks et al_1994_Effect of moderate dose of alcohol with evening meal on fibrinolytic factors.pdf},
