  year = {2016},
  keywords = {3D LAMINART model,border ownership,boundary grouping,cortical area V2,FACADE model,Figure-ground separation,Gestalt rules,Relative disparity,surface contour,surface filling-in},
  file = {/home/edunn/Documents/zotero/Frontiers in Psychology/2016/Grossberg_2016_Cortical Dynamics of Figure-Ground Separation in Response to 2D Pictures and 3D.pdf},
