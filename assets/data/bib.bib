  number = {10},
  urldate = {2017-07-27},
  url = {http://onlinelibrary.wiley.com.www2.lib.ku.edu/doi/10.1111/j.1478-3231.2012.02857.x/abstract},
  journal = {Liver International},
  author = {Jung, Tae Sik and Kim, Soo Kyoung and Shin, Hyun Joo and Jeon, Byeong Tak and Hahm, Jong Ryeal and Roh, Gu Seob},
  month = nov,
  year = {2012},
  keywords = {Diabetes,Non-alcoholic fatty liver disease,obesity,OLETF,Î±-Lipoic acid},
  pages = {1565--1573},
  file = {/media/truecrypt1/zotero/Liver International/2012/Jung et al_2012_Î±-lipoic acid prevents non-alcoholic fatty liver disease in OLETF rats.pdf;/home/edunn/Zotero/storage/7GJQR3ND/abstract.html}
}

