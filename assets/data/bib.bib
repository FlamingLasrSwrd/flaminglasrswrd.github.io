  journal = {International Journal of Sports Medicine},
  author = {Hammouda, O. and Chtourou, H. and Chahed, H. and Ferchichi, S. and Chaouachi, A. and Kallel, C. and Miled, A. and Chamari, K. and Souissi, N.},
