@article{YamashinaRolemitochondrialiver2009,
  series = {Mitochondrial MechanismsEndocrine Metabolism},
  title = {Role of Mitochondria in Liver Pathophysiology},
  volume = {6},
  issn = {1740-6765},
  doi = {10.1016/j.ddmec.2010.05.003},
  abstract = {Mitochondria play a central role in cellular energy homeostasis, metabolism, and cell death. Mitochondria generate energy via the electron transport chain located in the inner membrane for the synthesis of ATP. In the mitochondrial respiratory pathway, leakage of electrons induces the production of reactive oxygen species involved in the cellular aging and DNA damage. It is suggested that liver carcinogenesis, chronic hepatitis C, alcoholic liver disease and non-alcoholic fatty liver disease are the result of reactive oxygen species production because of mitochondrial dysfunction. The advancement of therapeutic approaches to protect mitochondria heralds a new strategy to halt progressive liver diseases and carcinogenesis.},
  number = {1},
  urldate = {2017-08-20},
  url = {http://www.sciencedirect.com/science/article/pii/S1740676510000052},
  journal = {Drug Discovery Today: Disease Mechanisms},
  author = {Yamashina, Shunhei and Sato, Nobuhiro and Kon, Kazuyoshi and Ikejima, Kenichi and Watanabe, Sumio},
  month = jan,
