  journal = {Biomolecules},
  author = {Szary, Nicholas and Rector, R. Scott and Uptergrove, Grace M. and Ridenhour, Suzanne E. and Shukla, Shivendra D. and Thyfault, John P. and Koch, Lauren G. and Britton, Steven L. and Ibdah, Jamal A.},
  month = nov,
  year = {2015},
  pages = {3295--3308},
  file = {/media/truecrypt1/zotero/Biomolecules/2015/Szary et al_2015_High Intrinsic Aerobic Capacity Protects against Ethanol-Induced Hepatic Injury.pdf},
  pmid = {26610588},
