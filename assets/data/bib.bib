  journal = {BioFactors},
  author = {Han, Derick and Handelman, Garry and Marcocci, Lucia and Sen, Chandan K. and Roy, Sashwati and Kobuchi, Hirotsugu and Tritschler, Hans J. and Floh{\'e}, Leopold and Packer, Lester},
  month = jan,
  year = {1997},
  keywords = {ASC transporter,cysteine uptake,Jurkat cells,peripheral blood lymphocytes,thiols,xâˆ’c transporter},
  pages = {321--338},
  file = {/home/edunn/Documents/zotero/BioFactors/1997/Han et al_1997_Lipoic acid increases de novo synthesis of cellular glutathione by improving.pdf;/home/edunn/Zotero/storage/KH3D4EQC/abstract.html}
