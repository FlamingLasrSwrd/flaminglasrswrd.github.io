  month = jul,
  year = {2012},
  pages = {1276--1286},
  file = {/media/truecrypt1/zotero/Biochimica et biophysica acta/2012/Clugston et al_2012_Altered hepatic retinyl ester concentration and acyl composition in response to.pdf},
  pmid = {23583843},
  pmcid = {PMC3786403}
}

