  file = {/media/truecrypt1/zotero/Environmental Health and Preventive Medicine/2016/Harada et al_2016_Metabolomic profiling reveals novel biomarkers of alcohol intake and.pdf},
  pmid = {26459263},
  pmcid = {PMC4693765}
}

