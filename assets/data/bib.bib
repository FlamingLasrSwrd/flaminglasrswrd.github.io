  month = mar,
  year = {2002},
  keywords = {Humans,Liver,Muscle; Skeletal,Glutathione,Apoptosis,Cytokines,Monocytes,Glutamine,Heat-Shock Proteins,Cell Cycle,Lymphocytes},
  pages = {217--221},
  pmid = {11882392}
}

