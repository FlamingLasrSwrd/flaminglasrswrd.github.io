  urldate = {2017-08-05},
  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2637578/},
  journal = {Biochemical Journal},
  author = {Mantena, Sudheer K. and Vaughn, Denty Paul and Andringa, Kelly K. and Eccleston, Heather B. and King, Adrienne L. and Abrams, Gary A. and Doeller, Jeannette E. and Kraus, David W. and Darley-Usmar, Victor M. and Bailey, Shannon M.},
  month = jan,
  year = {2009},
  pages = {183--193},
  file = {/media/truecrypt1/zotero/Biochemical Journal/2009/Mantena et al_2009_High fat diet induces dysregulation of hepatic oxygen gradients and.pdf},
  pmid = {18752470},
  pmcid = {PMC2637578}
}

