  pages = {177--187},
  file = {/home/edunn/Documents/zotero/Life Sciences/2007/Das_Vasudevan_2007_Alcohol-induced oxidative stress.pdf;/home/edunn/Zotero/storage/XNG5C6AW/S002432050700358X.html}
