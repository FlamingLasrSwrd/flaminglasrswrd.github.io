  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2768078/},
  journal = {Nutrition research (New York, N.Y.)},
  author = {Chung, Jayong and Veeramachaneni, Sudipta and Liu, Chun and Mernitz, Heather and Russell, Robert M. and Wang, Xiang-Dong},
  month = sep,
  year = {2009},
  pages = {664--670},
  file = {/media/truecrypt1/zotero/Nutrition research (New York, N.Y.)/2009/Chung et al_2009_Vitamin E supplementation does not prevent ethanol-reduced hepatic retinoic.pdf},
  pmid = {19854382},
