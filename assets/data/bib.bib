  keywords = {ethanol,Liver,Betaine,S-adenosylmethionine,Review,Lesion,Methionine synthetase},
  pages = {65--67},
  file = {/home/edunn/Documents/zotero/Alcohol/2002/Barak et al_2002_Methionine synthase.pdf}
}

