  title = {Ion {{Channels}} and {{Oxidative Stress}} as a {{Potential Link}} for the {{Diagnosis}} or {{Treatment}} of {{Liver Diseases}}},
  volume = {2016},
  doi = {10.1155/2016/3928714},
  abstract = {Oxidative stress results from a disturbed balance between oxidation and antioxidant systems. Reactive oxygen species (ROS) and reactive nitrogen species (RNS) may be either harmful or beneficial to the cells. Ion channels are transmembrane proteins that ...},
  language = {en},
  urldate = {2017-08-20},
