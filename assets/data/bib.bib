  author = {L{\"u}thi, Daniel and G{\"u}nzel, Dorothee and McGuigan, John A. S.},
  month = mar,
  year = {1999},
  pages = {231--252},
  file = {/home/edunn/Documents/zotero/Experimental Physiology/1999/LÃ¼thi et al_1999_Mg-Atp Binding.pdf;/home/edunn/Zotero/storage/344UGK8E/abstract.html}
}

