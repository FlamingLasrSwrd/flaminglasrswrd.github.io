  author = {Stone, M B and Merrick, M A and Ingersoll, C D and Edwards, J E},
  year = {2002},
  keywords = {Humans,Male,Time Factors,Adolescent,Adult,Analysis of Variance,Articular/ physiology,Bromelains/ therapeutic use,Dose-Response Relationship,Double-Blind Method,Drug,Drug Administration Schedule,Exercise/ physiology,Female,Follow-Up Studies,Ibuprofen/ therapeutic use,Muscle,Muscle Fatigue/physiology,Pain Measurement,Pain/diagnosis/ drug therapy,Probability,Range of Motion,Reference Values,Skeletal/drug effects/physiopathology,Treatment Outcome,Upper Extremity},
