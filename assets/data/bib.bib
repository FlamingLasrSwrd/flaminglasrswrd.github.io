  month = dec,
  year = {2013},
  keywords = {Cellular neuroscience,Cerebellum,Inhibition,Neurotransmitters},
  pages = {1783--1793},
  file = {/media/truecrypt1/zotero/Nature Neuroscience/2013/Kaplan et al_2013_Opposite actions of alcohol on tonic GABAA receptor currents mediated by nNOS.pdf;/home/edunn/Zotero/storage/8VFJ97TI/nn.3559.html}
}

