@article{NaleczCarnitinetransportphysiological2004,
  series = {Carnitine},
  title = {Carnitine: Transport and Physiological Functions in the Brain},
  volume = {25},
  issn = {0098-2997},
  shorttitle = {Carnitine},
  doi = {10.1016/j.mam.2004.06.001},
  abstract = {Carnitine (4-N-trimethylammonium-3-hydroxybutyric acid), a compound necessary for a transfer of fatty acids for their oxidation within the cell, accumulates in brain although $\beta$-oxidation of fatty acids is very low in neurons. Carnitine accumulates to lower extent in the brain than in peripheral tissues and the mechanism of its transport through the blood\textendash{}brain barrier is discussed, with the involvement of two transporters, OCTN2 and B0,+ being presented. A limitation by the blood\textendash{}brain barrier of carnitine supply for the brain and the mechanism of its transport to neural cells by a protein belonging to neurotransmitters' transporters superfamily is further discussed. Due to the beneficial effects of administration of acetylcarnitine in case of patients with dementia, the role of this acylcarnitine is presented in the context of neuronal cell metabolism and the role of acetylcarnitine in the synthesis of acetylcholine. The roles of long-chain acyl derivatives of carnitine, in particular palmitoylcarnitine, responsible for interaction with the membranes, lipids acylation and specific interactions with proteins have been summarized. Stimulation of protein palmitoylation and a possibility of changing the acylation status of G proteins is described, as well as interaction of palmitoylcarnitine with protein kinase C. Diminished interaction of the isoform $\delta$ of this kinase with GAP-43 (B-50, neuromodulin), whose expression increases upon accumulation of either carnitine or palmitoylcarnitine points to a possible regulation of differentiation by these compounds and their role in neuroregeneration.},
  number = {5},
  urldate = {2017-07-21},
  url = {http://www.sciencedirect.com/science/article/pii/S0098299704000469},
  journal = {Molecular Aspects of Medicine},
  author = {Na{\l}\k{e}cz, Katarzyna A. and Miecz, Dorota and Berezowski, Vincent and Cecchelli, Rom{\'e}o},
  month = oct,
  year = {2004},
  keywords = {B,Carnitine,OCTN2,Palmitoylation,Protein kinase C},
  pages = {551--567},
  file = {/home/edunn/Documents/zotero/Molecular Aspects of Medicine/2004/NaÅ‚Ä™cz et al_2004_Carnitine.pdf;/home/edunn/Zotero/storage/Q6WZC8MU/S0098299704000469.html},
  annote = {Carnitine (4-N-trimethylammonium-3-hydroxybutyric acid) is known to be involved in peripheral tissues in a process of long-chain fatty acid transfer from the cytosol to the mitochondrial matrix and to peroxisomes, where these acids are further metabolized.

In neurons isolated from adult brain about 80\% of total carnitine remains in free form, while about 10\textendash{}15\% is in form of acetylcarnitine.

In neurons isolated from suckling rats an increase of the amount of acetylcarnitine is observed, whilst the amount of free carnitine diminishes by 40\% in comparison with the adult brain (Wawrze{\'n}czyk et al., 1995b)

Acetylcarnitine is able to cross the blood\textendash{}brain barrier by OCTN2 (Inano et al., 2003; Kido et al., 2001) and is also synthesized in neurons (Wawrze{\'n}czyk et al., 1995b).

It was, however, demonstrated that citrate pathway supplied not more than 30\% of acetyl moieties for acetylcholine synthesis in mammalian brain (Gibson and Shimada, 1980; Sterling and O'Neill, 1978; Tu{\v c}ek et al., 1981).

The works of Tu{\v c}ek and his collaborators demonstrated that acetylcarnitine can be a source of acetyl groups for acetylcholine synthesis (Dole{\v z}al and Tu{\v c}ek, 1981; Tu{\v c}ek, 1985, 1990), moreover being a better precursor than citrate (Tu{\v c}ek and Cheng, 1970, 1974; Tu{\v c}ek et al., 1981).

It was also shown that upon depletion of acetylcholine content, synthesis of this neurotransmitter was stimulated by addition of glucose, carnitine or acetylcarnitine itself, leading to a conclusion that all these compounds could increase the availability of acylCoA (Ri{\v c}ny et al., 1992; Tu{\v c}ek et al., 1982; Tu{\v c}ek, 1983).}
}

@article{PekalaLCarnitineMetabolicFunctions2011,
  title = {L-{{Carnitine}} - {{Metabolic Functions}} and {{Meaning}} in {{Humans Life}}},
  volume = {12},
  issn = {13892002},
  doi = {10.2174/138920011796504536},
  language = {en},
  number = {7},
  urldate = {2017-07-21},
  url = {http://www.eurekaselect.com/openurl/content.php?genre=article\&issn=1389-2002\&volume=12\&issue=7\&spage=667},
  journal = {Current Drug Metabolism},
  author = {Pekala, Jolanta and Patkowska-Sokola, Bozena and Bodkowski, Robert and Jamroz, Dorota and Nowakowski, Piotr and Lochynski, Stanislaw and Librowski, Tadeusz},
  month = sep,
  year = {2011},
  pages = {667--678},
  file = {/media/truecrypt1/zotero/Current Drug Metabolism/2011/Pekala et al_2011_L-Carnitine - Metabolic Functions and Meaning in Humans Life.pdf},
  annote = {Under certain conditions, the demand for L\textemdash{}carnitine may exceed an individual's capacity to synthesize it, making it a conditionally essential micronutrient [4,5].

D\textemdash{}carnitine is not only physiologically inactive (does not drive metabolism of fatty acid) but also has a considerable toxic influence on biochemical processes. Moreover D\textemdash{}carnitine is bound and transported by the active L-carnitine transport system of cell membranes, thereby diminishing the L-camitine within the cell and inhibiting L\textemdash{}specific reactions [6,7].

Because the carnitine concentration in tissues is generally 2|]- 50-fold higher than in plasma, and since, in humans, only the kid- ney, liver and brain have the complete set of enzymes to synthesize carnitine, most tissues depend on carnitine uptake from the blood via active transport.

More than 90\% ofthe total carnitine pool resides within skeletal and myocardial muscle, with the remainder being present in the liver, kidneys and other tissues [35].

The daily requirement for L-camitine amounts ca 15 mg and is supplied to the body through endogenous biosynthesis and exoge\textemdash{} nous sources.

Under normal circum- stances, the adult human body (\textasciitilde{}TU kg) can synthesize from 11 to 34 mg L-carnitine per day (160-480 pgi'kg body weight).

A properly constructed diet (in omnivorous humans) provides about 75 percent of the daily carnitine requirement and only 25 percent comes from endogenous synthesis.

The primary source of L-carnitine is red meat, particularly lamb and beef, while a lower carnitine content is found in fish, pork and poultry as well as in dairy products such as whole milk or cottage cheese. Plant origin products generally do not comprise good sources of carnitine. In fruit and vegetables L-carnitine occurs just in trace quantities (or does not occur at all).

However, this quantum can be insufficient, when it comes to stress and physi- cal exertion, especially in the case of advanced training sportsmen or athletes.

Daily losses are minimal ($<$60mg/day) and are reduced to less than 20 mg/day on a meat\textemdash{} and carnitine-free diet.

The primary function of L-carnitine is to permit the entry of esterified fatty acids (the source for the ATP synthesis) into the mitochondrial matrix, where [Bi-oxidation occurs.

Long-chain fatty acid acyl groups are transported exclusively as carnitine esters by a carnitine carrier called the translocase, which constitutes as a transmembraneous protein in the inner mitochondrial membrane.

The carnitine system has evolved to allow their transport across these membranes and thus facilitates their specific metabolic roles, This is made possible by the presence of carnitinelacylcarnitine transporters on cellular membranes, which shuttles these derivatives between intracellular compartments.

Carnitine lowers the ratio of intrarnitochondrial acyl-CoA to CoA and causes detoxi ï€ cation of accumulated acyl-CoA esters in patients with a defective metabolism of glucose or acyl-oxidation.

~}
}

