  year = {2013},
  pages = {137--145},
  file = {/media/truecrypt1/zotero/Sleep/2013/Van Reen et al_2013_Biphasic Effects of Alcohol as a Function of Circadian Phase.pdf},
  pmid = {23288980},
  pmcid = {PMC3524536}
}

