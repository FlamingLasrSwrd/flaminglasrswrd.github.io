  urldate = {2017-01-09},
  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3022940/},
  journal = {Sleep},
  author = {Van Reen, Eliza and Tarokh, Leila and Rupp, Tracy L. and Seifer, Ron and Carskadon, Mary A.},
  month = feb,
  year = {2011},
  pages = {195--205},
  file = {/media/truecrypt1/zotero/Sleep/2011/Van Reen et al_2011_Does Timing of Alcohol Administration Affect Sleep.pdf},
  pmid = {21286495},
  pmcid = {PMC3022940}
}

