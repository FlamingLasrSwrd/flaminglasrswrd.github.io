  urldate = {2018-01-21},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3174857/},
  journal = {The Journal of Nutrition},
  author = {Fulgoni, Victor L. and Keast, Debra R. and Bailey, Regan L. and Dwyer, Johanna},
  month = oct,
  year = {2011},
  pages = {1847--1854},
  file = {/home/edunn/Documents/zotero/The Journal of Nutrition/2011/Fulgoni et al_2011_Foods, Fortificants, and Supplements.pdf},
  pmid = {21865568},
