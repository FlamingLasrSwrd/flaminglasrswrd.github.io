  urldate = {2018-02-22},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4739591/},
  journal = {PLoS ONE},
  author = {Elorinne, Anna-Liisa and Alfthan, Georg and Erlund, Iris and Kivim{\"a}ki, Hanna and Paju, Annukka and Salminen, Irma and Turpeinen, Ursula and Voutilainen, Sari and Laakso, Juha},
