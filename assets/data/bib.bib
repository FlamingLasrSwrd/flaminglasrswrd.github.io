  journal = {World Journal of Gastroenterology : WJG},
  author = {Bolkent, Sema and Arda-Pirincci, Pelin and Bolkent, Sehnaz and Yanardag, Refiye and Tunali, Sevim and Yildirim, Sukriye},
  month = jul,
