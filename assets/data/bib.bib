  author = {Koksal, M. and Kurcer, Z. and Erdogan, D. and Iraz, M. and Tas, M. and Eren, M. A. and Aydogan, T. and Ulas, T.},
  month = apr,
  year = {2012},
  keywords = {Animals,Rats,Liver,Acetylcysteine,Chemical and Drug Induced Liver Injury,cytoprotection,Disease Models; Animal,Male,Melatonin,Methanol,Microscopy; Electron,Protective Agents,Rats; Wistar,Time Factors},
  pages = {437--444},
  file = {/media/truecrypt1/zotero/European Review for Medical and Pharmacological Sciences/2012/Koksal et al_2012_Effect of melatonin and n-acetylcysteine on hepatic injury in rat induced by.pdf},
  pmid = {22696870}
