  journal = {Journal of Applied Physiology},
  author = {Zuhl, Micah N. and Lanphere, Kathryn R. and Kravitz, Len and Mermier, Christine M. and Schneider, Suzanne and Dokladny, Karol and Moseley, Pope L.},
