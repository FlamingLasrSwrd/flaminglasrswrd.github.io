  url = {http://jap.physiology.org/content/95/1/145},
  journal = {Journal of Applied Physiology},
  author = {Hiscock, N. and Petersen, E. W. and Krzywkowski, K. and Boza, J. and Halkjaer-Kristensen, J. and Pedersen, B. K.},
  month = jul,
