  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3068843/},
  journal = {Brain research},
  author = {Antonio, Angeline M. and Gillespie, Roberta A. and Druse, Mary J.},
  month = apr,
  year = {2011},
  pages = {13--21},
  file = {/media/truecrypt1/zotero/Brain research/2011/Antonio et al_2011_Effects of Lipoic Acid on Antiapoptotic Genes in Control and Ethanol-Treated.pdf},
  pmid = {21303669},
  pmcid = {PMC3068843}
}

