@article{sivaraj_characterization_1988,
  title = {Characterization of Copper/Alumina Catalysts Prepared by Deposition\textemdash{}precipitation Using Urea Hydrolysis: {{I}}. {{Nitrous Oxide Decomposition}} and {{Reaction}} of {{Ethanol}}},
  volume = {45},
  issn = {0166-9834},
  shorttitle = {Characterization of Copper/Alumina Catalysts Prepared by Deposition\textemdash{}precipitation Using Urea Hydrolysis},
  doi = {10.1016/S0166-9834(00)82396-9},
  abstract = {Copper\textemdash{}alumina catalysts with a wide range of copper loadings (4 to 34 wt.-\%) were prepared by depositing Cu2+ on suspended $\gamma$-alumina through precipitation from a homogeneous solution using urea hydrolysis at 90\textendash{}95$^\circ$C. The effect of copper content on the crystallographic phase and BET surface area of the catalysts calcined at 400$^\circ$C for 24 h was investigated. Catalysts containing $\leq$16 wt.-\% copper did not show any copper containing phase indicating the presence of highly dispersed or amorphous copper species. These catalysts have shown high copper metal areas ranging from 308 to 108 m2/g-Cu as determined by the reaction of nitrous oxide with copper surfaces at 90$^\circ$C. While catalysts with low copper content exhibited both dehydration and dehydrogenation activity, those with high copper content showed only dehydrogenation activity at reaction temperatures of 250\textendash{}300$^\circ$C. The selectivities for dehydrogenation products were correlated with copper areas of the catalysts.},
  number = {1},
  urldate = {2017-11-16},
  url = {http://www.sciencedirect.com/science/article/pii/S0166983400823969},
  journal = {Applied Catalysis},
  author = {Sivaraj, Ch. and Kantarao, P.},
  month = jan,
  year = {1988},
  pages = {103--114},
  file = {/home/edunn/Documents/zotero/Applied Catalysis/1988/Sivaraj_Kantarao_1988_Characterization of copper-alumina catalysts prepared by.pdf;/home/edunn/Zotero/storage/Z35TRVF2/S0166983400823969.html},
  annote = {Copper (II ) nitrate (Fluka) and urea (Ranbaxy ) were of analytical reagent grade. Al203 used as support was prepared from basic aluminium succinate precipitated from a homogeneous solution according to the procedure de- scribed earlier [9]. Hydrogen, nitrous oxide and nitrogen gases were of IOL AR grade (Indian Oxygen Limited) and carbon monoxide was of Matheson make, all of 99.9\% purity.

An aqueous solution (2 1) containing copper (II) nitrate of required concentration and urea (1.2 M) was placed in a 3-1 round bottom glass reactor, 20 g of y-alumina (-200 mesh) was then added and heated to 90-95 ' C with constant stirring. The pH of the solution was measured at. different intervals of time using a digital pH meter. After the precipitation was complete (pH = 7.5 ), the solution was filtered in a Buchner funnel, the precipitate washed several times with distilled water and dried at 110' C for 24 h. The supported oxide catalysts were then obtained by calcining the catalyst precursors at 400' C for 24 h. The copper content of the calcined Cu-A\&O, catalysts varied bet.ween 4 and 34\% wt.-\%.}
}

