  journal = {Journal of Gastroenterology and Hepatology},
  author = {Jiang, Qian and Jiang, Xue-Hua and Zheng, Ming-Hua and Chen, Yong-Ping},
  month = jan,
  year = {2009},
  keywords = {treatment,Hepatic Encephalopathy,l-ornithine-l-aspartate,meta-analysis,randomized controlled trial},
  pages = {9--14},
  file = {/media/truecrypt1/zotero/Journal of Gastroenterology and Hepatology/2009/Jiang et al_2009_l-Ornithine-l-aspartate in the management of hepatic encephalopathy.pdf;/home/edunn/Zotero/storage/X4FHTPXK/abstract.html}
}

