}

@article{harris_achieving_2014,
  title = {Achieving Optimal N\textendash{}3 Fatty Acid Status: The Vegetarian's Challenge... or Not\textendash{}},
  volume = {100},
  issn = {0002-9165},
  shorttitle = {Achieving Optimal N\textendash{}3 Fatty Acid Status},
  doi = {10.3945/ajcn.113.071324},
  abstract = {The long chain n\textendash{}3 (omega-3) fatty acids eicosapentaenoic acid (EPA) and docosahexaenoic acid (DHA), although originally synthesized by microorganisms in the oceans, are primarily obtained from the consumption of fish. Vegetarians, by definition, do not eat fish and thus consume virtually no EPA and DHA. Because conversion of the plant-derived n\textendash{}3 fatty acid $\alpha$-linolenic acid (ALA) to EPA and DHA is very low, n\textendash{}3 tissue concentrations in vegetarians are lower than in omnivores. This review asks 2 questions: what is the evidence that increased n\textendash{}3 concentrations reduce the risk of cardiovascular disease in vegetarians, and, if it does, how can vegetarians increase their blood and tissue concentrations of these animal-derived fatty acids? At present, both cardiovascular risk markers and cardiovascular events appear to be significantly reduced in vegetarians compared with those in omnivores. If so, and in the absence of data to show that risk in vegetarians could be even lower with higher n\textendash{}3 concentrations, then the second question becomes moot. However, the absence of evidence is not evidence of absence; therefore, at our present state of knowledge, increasing n\textendash{}3 concentrations is not an unreasonable goal for vegetarians. This can be accomplished by a variety of approaches, including increased intakes of ALA, consumption of stearidonic acid\textendash{}enriched soybean oil (if and when it comes to the market), and the use of supplements containing EPA, DHA, or both derived from nonanimal sources (microalgae, biotech yeast, and, in the future, biotech plant oils).},
  language = {en},
  number = {suppl\_1},
  urldate = {2018-02-04},
  url = {https://academic.oup.com/ajcn/article/100/suppl_1/449S/4576612},
  journal = {The American Journal of Clinical Nutrition},
  author = {Harris, William S.},
  month = jul,
  year = {2014},
  pages = {449S--452S},
  file = {/home/edunn/Documents/zotero/The American Journal of Clinical Nutrition/2014/Harris_2014_Achieving optimal nâ€“3 fatty acid status.pdf;/home/edunn/Zotero/storage/844M6G7F/4576612.html},
  note = {00015}
}

@article{sanders_plant_2014,
  title = {Plant Compared with Marine N\textendash{}3 Fatty Acid Effects on Cardiovascular Risk Factors and Outcomes: What Is the Verdict?\textendash{}},
  volume = {100},
  issn = {0002-9165},
  shorttitle = {Plant Compared with Marine N\textendash{}3 Fatty Acid Effects on Cardiovascular Risk Factors and Outcomes},
  doi = {10.3945/ajcn.113.071555},
  abstract = {Plants provide $\alpha$-linolenic acid [ALA; 18:3n\textendash{}3 (18:3$\omega$-3)], which can be converted via eicosapentaenoic acid (EPA; 20:5n\textendash{}3) to docosahexaenoic acid (DHA; 22:6n\textendash{}3), which is required for normal visual and cognitive function. Dietary ALA is provided mainly by vegetable oils, especially soybean and rapeseed oils, but is destroyed by partial hydrogenation; it is also present in high amounts in walnuts and flaxseed. Dietary EPA and DHA are provided mainly by fish and so are absent from vegan diets and only present in trace amounts in vegetarian diets. Vegetarians and vegans have lower proportions of DHA in blood and tissue lipids compared with omnivores. High intakes of EPA and DHA (typically in the range of 3\textendash{}5 g/d) but not ALA have favorable effects on several cardiovascular disease (CVD) risk factors and have been postulated to delay arterial aging and cardiovascular mortality, but these intakes are beyond the range of normal dietary intake. Arterial stiffness, which is a measure of arterial aging, appears to be lower in vegans than in omnivores; and risk of CVD in vegetarians and vegans is approximately one-third that in omnivores. Prospective cohort studies showed higher intakes of EPA+DHA, and less consistently ALA, to be associated with a lower risk of CVD, especially fatal coronary heart disease, but meta-analyses of randomized controlled trials of supplementation of EPA+DHA or ALA in secondary prevention of CVD showed no clear benefit. Current evidence is insufficient to warrant advising vegans and vegetarians to supplement their diets with EPA or DHA for CVD prevention.},
  language = {en},
  number = {suppl\_1},
  urldate = {2018-02-04},
  url = {https://academic.oup.com/ajcn/article/100/suppl_1/453S/4576630},
  journal = {The American Journal of Clinical Nutrition},
  author = {Sanders, Thomas AB},
  month = jul,
  year = {2014},
  pages = {453S--458S},
  file = {/home/edunn/Documents/zotero/The American Journal of Clinical Nutrition/2014/Sanders_2014_Plant compared with marine nâ€“3 fatty acid effects on cardiovascular risk.pdf;/home/edunn/Zotero/storage/MT4Q5FEM/4576630.html},
  note = {00018}
}

