  month = nov,
  year = {2008},
  pages = {1477--1486},
  file = {/media/truecrypt1/zotero/Hepatology/2008/Park et al_2008_Alpha-lipoic acid decreases hepatic lipogenesis through adenosine.pdf;/home/edunn/Zotero/storage/5772ZRHT/abstract.html}
