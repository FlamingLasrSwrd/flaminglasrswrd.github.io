  url = {http://doi.wiley.com/10.1002/14651858.CD009374.pub3},
  booktitle = {Cochrane {{Database}} of {{Systematic Reviews}}},
  publisher = {{John Wiley \& Sons, Ltd}},
  author = {Chen, Ning and Yang, Mi and Zhou, Muke and Xiao, Jing and Guo, Jian and He, Li},
