  url = {https://www.cambridge.org/core/journals/british-journal-of-nutrition/article/brain-creatine-depletion-in-vegetarians-a-crosssectional-1hmagnetic-resonance-spectroscopy-1hmrs-study/C01E9E1F222136316801626F6C352B8F},
