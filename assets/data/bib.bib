  month = jan,
  year = {2016},
  keywords = {Betaine,NAFLD,Dietary fructose,Hepatic ER stress,LXRÎ± and PPARÎ±},
  pages = {154--164},
  file = {/media/truecrypt1/zotero/European Journal of Pharmacology/2016/Ge et al_2016_Betaine prevented fructose-induced NAFLD by regulating LXRÎ±-PPARÎ± pathway and.pdf;/home/edunn/Zotero/storage/P3ESQUGD/S0014299915303800.html}
}

