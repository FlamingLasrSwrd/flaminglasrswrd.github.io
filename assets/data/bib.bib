  journal = {Pharmacology},
  author = {Sehirli, Ozer and Tatlidede, Elif and Y{\"u}ksel, Meral and Erzik, Can and Cetinel, Sule and Ye{\u g}en, Berrak C. and Sener, G{\"o}ksel},
  year = {2008},
  keywords = {Animals,ethanol,Rats,Male,Rats; Wistar,Lipid Peroxidation,Female,Gastric Mucosa,Stomach Ulcer,Antioxidants,Reactive Oxygen Species,Thioctic Acid},
  pages = {173--180},
  file = {/media/truecrypt1/zotero/Pharmacology/2008/Sehirli et al_2008_Antioxidant effect of alpha-lipoic acid against ethanol-induced gastric mucosal.pdf},
