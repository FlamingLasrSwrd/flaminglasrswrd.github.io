  journal = {Alcoholism: Clinical and Experimental Research},
  author = {Thomes, Paul G. and Trambly, Casey S. and Fox, Howard S. and Tuma, Dean J. and Donohue, Terrence M.},
  month = dec,
  year = {2015},
  keywords = {Steatosis,Autophagosome,Lysosome,Proteopathy,Transcription Factor EB},
  pages = {2354--2363},
  file = {/media/truecrypt1/zotero/Alcoholism Clinical and Experimental Research/2015/Thomes et al_2015_Acute and Chronic Ethanol Administration Differentially Modulate Hepatic.pdf;/home/edunn/Zotero/storage/S3A2T8KQ/abstract.html;/home/edunn/Zotero/storage/VM9RSKZG/abstract.html}
}

