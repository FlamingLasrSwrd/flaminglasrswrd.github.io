  month = oct,
  year = {2009},
  keywords = {Lipid Peroxidation,Superoxide Dismutase,Hippocampus,Lipoic acid,Monoamines},
  pages = {131--134},
  file = {/media/truecrypt1/zotero/Neuroscience Letters/2009/Ferreira et al_2009_Lipoic acid effects on lipid peroxidation level, superoxide dismutase activity.pdf;/home/edunn/Zotero/storage/VCN2CGHB/S0304394009011392.html}
}

@article{ValdecantosLipoicacidadministration2012,
