@article{RumpAcetylLcarnitineprotectsneuronal2010,
  title = {Acetyl-{{L}}-Carnitine Protects Neuronal Function from Alcohol-Induced Oxidative Damage in the Brain},
  volume = {49},
  issn = {0891-5849},
  doi = {10.1016/j.freeradbiomed.2010.08.011},
  abstract = {The studies presented here demonstrate the protective effect of acetyl-L-carnitine (ALC) against alcohol-induced oxidative neuroinflammation, neuronal degeneration, and impaired neurotransmission. Our findings reveal the cellular and biochemical mechanisms of alcohol-induced oxidative damage in various types of brain cells. Chronic ethanol administration to mice caused an increase in inducible nitric oxide synthase (iNOS) and 3-nitrotyrosine adduct formation in frontal cortical neurons but not in astrocytes from brains of these animals. Interestingly, alcohol administration caused a rather selective activation of NADPH oxidase (NOX), which, in turn, enhanced levels of reactive oxygen species (ROS) and 4-hydroxynonenal, but these were predominantly localized in astrocytes and microglia. Oxidative damage in glial cells was accompanied by their pronounced activation (astrogliosis) and coincident neuronal loss, suggesting that inflammation in glial cells caused neuronal degeneration. Immunohistochemistry studies indicated that alcohol consumption induced different oxidative mediators in different brain cell types. Thus, nitric oxide was mostly detected in iNOS-expressing neurons, whereas ROS were predominantly generated in NOX-expressing glial cells after alcohol ingestion. Assessment of neuronal activity in ex vivo frontal cortical brain tissue slices from ethanol-fed mice showed a reduction in long-term potentiation synaptic transmission compared with slices from controls. Coadministration of ALC with alcohol showed a significant reduction in oxidative damage and neuronal loss and a restoration of synaptic neurotransmission in this brain region, suggesting that ALC protects brain cells from ethanol-induced oxidative injury. These findings suggest the potential clinical utility of ALC as a neuroprotective agent that prevents alcohol-induced brain damage and development of neurological disorders.},
  number = {10},
  urldate = {2017-06-23},
  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3022478/},
  journal = {Free radical biology \& medicine},
  author = {Rump, Travis J. and Muneer, P.M. Abdul and Szlachetka, Adam M. and Lamb, Allyson and Haorei, Catherine and Alikunju, Saleena and Xiong, Huangui and Keblesh, James and Liu, Jianuo and Zimmerman, Matthew C. and Jones, Jocelyn and Donohue, Terrence M. and Persidsky, Yuri and Haorah, James},
  month = nov,
  year = {2010},
  pages = {1494--1504},
  file = {/media/truecrypt1/zotero/Free radical biology & medicine/2010/Rump et al_2010_Acetyl-L-carnitine protects neuronal function from alcohol-induced oxidative.pdf},
  pmid = {20708681},
  pmcid = {PMC3022478},
  annote = {\textbf{Extracted Annotations (7/21/2017, 12:55:18 PM)}

"Coadministration of ALC with alcohol showed a significant reduction in oxidative damage and neuronal loss and a restoration of synaptic neurotransmission in this brain region, suggesting that ALC protects brain cells from ethanol-induced oxidative injury." (\href{zotero://open-pdf/0_PQGVVQJB/1}{Rump et al 2010:1494})

"These findings support the notion that oxidant generation is a cause of neuroinflammation and neuronal degeneration in alcoholics." (\href{zotero://open-pdf/0_PQGVVQJB/2}{Rump et al 2010:1495})

"We and others have demonstrated the biochemical and molecular mechanisms of alcohol-induced oxidative production as contributing factors to neuronal degeneration after alcohol-induced breaching of brain microvascular endothelial cells [15,16], brain astrocytes [17,18], and cultured microglia [19]." (\href{zotero://open-pdf/0_PQGVVQJB/2}{Rump et al 2010:1495})

"We reported a significant induction of cytochrome P450-2E1 activity, which was associated with increased levels of reactive oxygen species (ROS) and nitric oxide (NO) production in human neurons after exposure to 17.5 mM ethanol (EtOH) [17]." (\href{zotero://open-pdf/0_PQGVVQJB/2}{Rump et al 2010:1495})

"These findings indicate that ethanol metabolism and subsequent reactive oxidant production by both NADPH oxidase (NOX) and inducible nitric oxide synthase (iNOS) significantly contribute to oxidative stress in the central nervous system (CNS)." (\href{zotero://open-pdf/0_PQGVVQJB/2}{Rump et al 2010:1495})

"In this study, we investigated the therapeutic efficacy of acetyl-L-carnitine (ALC) administration in a mouse model of chronic alcohol feeding." (\href{zotero://open-pdf/0_PQGVVQJB/2}{Rump et al 2010:1495})

"It is an effective antioxidant and neurotransmitter and a potent inhibitor of excitotoxicity." (\href{zotero://open-pdf/0_PQGVVQJB/2}{Rump et al 2010:1495})

"Statistical significance was assessed by two-way ANOVA analyses with Newman-Keuls posttest for multiple comparisons." (\href{zotero://open-pdf/0_PQGVVQJB/5}{Rump et al 2010:1498})

"As anticipated, ALC prevented the alcohol-elicited loss of LTP and LFS synaptic transmission in the frontal cortex, suggesting that ALC blocked the loss of neuronal activity, perhaps by stabilizing mitochondrial function." (\href{zotero://open-pdf/0_PQGVVQJB/5}{Rump et al 2010:1498})

"These data are consistent with our recent findings that, in culture, ALC exposure protects primary human neurons from EtOH/Acetaldehyde (Ach) insult [17]." (\href{zotero://open-pdf/0_PQGVVQJB/5}{Rump et al 2010:1498})

"The data indicated that ethanol administration diminished NF fluorescence in cortical sections." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"However, daily coadministration of ALC with ethanol liquid diet prevented the loss of NF from cortical neurons in these animals (Figs. 2A-C)." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"The mitochondrial-specific antioxidant CoQ10 afforded very minimal neuroprotection from chronic alcohol administration (data not shown), similar to our unpublished data in cell culture studies, which was attributed to the impermeability of exogenous CoQ10 across the cellular and mitochondrial membranes." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"Our findings revealed that dopaminergic neurons, mostly localized in the substantia nigra region, exhibited degeneration after ethanol administration (Figs. 2D-F)." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"Coadministration of ALC seemed to protect both dopaminergic and cholinergic neurons from alcohol insults in this region." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"In agreement with our recent in vitro findings, we observed a twofold increase in the intensity of the 135-kDa iNOS protein in the frontal cortex after chronic alcohol ingestion compared with controls (Fig. 3A)." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"Coadministration of ALC with ethanol for 8 weeks significantly decreased alcohol-elicited iNOS protein induction in this brain region (Figs. 3A and B)." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"Interestingly, we observed fewer neurons in brain tissues from the EtOH condition compared with the control or the ALC + EtOH condition (Figs. 4E and F)." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"We then examined the levels of 3-nitrotyrosine protein adducts (3-NT immunoreactivity is a signature of peroxynitrite binding to proteins)." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"We found that ethanol administration caused a 2.3-fold increase in the 3-NT protein level," (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"Unlike CoQ10, administration of ALC significantly blocked the ethanol-elicited up-regulation of 3-NT protein to levels that approached those of controls. These results confirm our recent findings that EtOH/Ach caused a 45\% induction of iNOS protein level in primary human neurons [17]." (\href{zotero://open-pdf/0_PQGVVQJB/6}{Rump et al 2010:1499})

"Again, ALC treatment partially neutralized the effects of alcohol on NOX1 protein expression in astrocytes and microglia." (\href{zotero://open-pdf/0_PQGVVQJB/7}{Rump et al 2010:1500})

"Ethanol administration caused a 2.5-fold increase in the intensity of the 65-kDa NOX1-immunoreactive band in brain tissues compared with brains from pairfed controls (Fig. 9A). Elevation in NOX1 protein level was accompanied by a 2.8-fold increase in 4-HNE protein adduct content in alcohol-fed mice compared with controls (Fig. 9B)." (\href{zotero://open-pdf/0_PQGVVQJB/7}{Rump et al 2010:1500})

"Coadministration of ALC (but not malonyl coenzyme A or CoQ10) almost completely abrogated the up-regulation by alcohol of NOX1 and 4-HNE protein in the brain." (\href{zotero://open-pdf/0_PQGVVQJB/7}{Rump et al 2010:1500})

"Thus, our findings indicate that ALC neutralizes oxidative damage in the brain caused by chronic ethanol administration." (\href{zotero://open-pdf/0_PQGVVQJB/7}{Rump et al 2010:1500})

"This concentration is considered a high ethanol dose in mice. Blood alcohol levels were 10.6-17.6 mM (0.05-0.1\%)." (\href{zotero://open-pdf/0_PQGVVQJB/7}{Rump et al 2010:1500})

"The significance of the present findings is that chronic alcohol feeding activated ROS and RNS-generating pathways in specific brain cell types." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"We noted that timely administration of ALC significantly stabilized the long-term synaptic neurotransmission of dopaminergic and cholinergic neurons by preventing the activation of iNOS, NOX, and glial cells." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"These findings indicate that chronic alcohol ingestion preferentially modulates iNOS protein level in neurons but not in astrocytes or microglia, validating our recent findings that EtOH/Ach exposure increased the level of iNOS protein in cultured primary human neurons [17]." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"We conclude that iNOS induction led to accumulation of NO and peroxynitrite formation thereby promoting the transduction of oxidative stress and neuronal degeneration during chronic ethanol consumption." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"This implies that NO generated by neurons diffuses into glial cells and reacts with superoxide to form highly reactive peroxynitrite." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"Most importantly, ALC significantly minimized the level of alcohol-induced iNOS/nitrotyrosine adduct formation in neurons and further prevented NOX protein induction in astrocytes and microglia, thereby preventing gliosis in the frontal cortex." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"Thus, we postulate that ALC exerts its antioxidant property by indirect mechanisms such as its ability to maintain mitochondrial membrane potential transition." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"One possible mechanism by which ALC could act is by suppressing the activation of free radical-generating pathways such as iNOS and NOX as indicated by the present findings." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"Another plausible mechanism for ALC to circumvent the excessive level of oxidative stress is to stabilize the naturally occurring biological antioxidant enzymes and antioxidants such as SOD, catalase, or the intracellular levels of glutathione." (\href{zotero://open-pdf/0_PQGVVQJB/8}{Rump et al 2010:1501})

"Such was the case in this study, that the numbers of microglia in alcoholic brain were higher than those in mice given ALC plus alcohol or in the pair-fed controls" (\href{zotero://open-pdf/0_PQGVVQJB/9}{Rump et al 2010:1502})

"This finding suggested that, although ALC provided neuroprotection, it was unable to inhibit the sensitivity to chronic alcohol insult completely." (\href{zotero://open-pdf/0_PQGVVQJB/9}{Rump et al 2010:1502})

"In particular, the ability of ALC to prevent alcohol-induced mitochondrial damage, energy depletion due to defective respiratory chain complexes, oxidative-mediated neuroinflammation, and neuronal degeneration will be of significant importance in further investigations." (\href{zotero://open-pdf/0_PQGVVQJB/9}{Rump et al 2010:1502})

"Indeed, during chronic alcohol consumption, ALC sustained the neuronal LFS and LTP synaptic transmission by preserving the integrity of neurofilaments and by maintaining dopaminergic and cholinergic neurons." (\href{zotero://open-pdf/0_PQGVVQJB/9}{Rump et al 2010:1502})

"These findings lead us to suggest that ALC regulates the activation of the acetylcholine receptor, as the acetyl group is an essential precursor for the neurotransmitter acetylcholine." (\href{zotero://open-pdf/0_PQGVVQJB/9}{Rump et al 2010:1502})

"we propose that daily use of dietary ALC (1-2 mg/kg body wt) by alcohol abusers could provide a potential health benefit." (\href{zotero://open-pdf/0_PQGVVQJB/9}{Rump et al 2010:1502})}
}

