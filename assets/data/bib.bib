  month = dec,
  year = {2015},
  keywords = {NADPH oxidase,Laminar sheer stress,Nitric oxide,Nitric oxide synthase,Superoxide,Tetrahydrobiopterin},
  pages = {1079--1086},
  file = {/home/edunn/Documents/zotero/Nutrition, Metabolism and Cardiovascular Diseases/2015/Clifton_2015_From sodium intake restriction to nitrate supplementation.pdf;/home/edunn/Zotero/storage/HN759N59/S0939475315300041.html},
